{
  "name": "reciple",
  "description": "Reciple is a Discord.js bot framework",
  "homepage": "https://reciple.js.org/docs/reciple",
  "license": "GPL-3.0",
  "version": "7.9.9",
  "main": "./bin/index.js",
  "module": "./bin/esm.mjs",
  "types": "./bin/index.d.ts",
  "bin": "./bin/bin.mjs",
  "keywords": [
    "reciple",
    "discord.js",
    "handler",
    "commands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FalloutStudios",
    "directory": "packages/reciple"
  },
  "exports": {
    ".": {
      "types": "./bin/index.d.ts",
      "import": "./bin/esm.mjs",
      "require": "./bin/index.js"
    }
  },
  "scripts": {
    "clean": "npx rimraf ./bin",
