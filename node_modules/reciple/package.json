    "fallout-utility": "^2.7.1",
    "globby": "^13.2.1",
    "lodash.mergewith": "^4.6.2",
    "micromatch": "^4.0.5",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/lodash.mergewith": "^4.6.7",
    "@types/micromatch": "^4.0.2",
